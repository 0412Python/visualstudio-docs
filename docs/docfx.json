{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "group-vs",
        "src": ".",
        "exclude": [
          "**/obj/**",
          "vsdocsdocs/**",
          "**/includes/**",
          "missingapi.yml",
          "vs-2015/**"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "group": "group-vs2015",
        "src": "./vs-2015",
        "exclude": [
          "**/obj/**",
          "vsdocsdocs/**",
          "**/includes/**",
          "missingapi.yml"
        ]
      }
        ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "group": "group-vs",
        "src": ".",
        "exclude": [
          "**/obj/**",
          "vsdocsdocs/**",
          "**/includes/**",
          "vs-2015/**"
        ]
      },
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "group": "group-vs2015",
        "src": "vs-2015",
        "exclude": [
          "**/obj/**",
          "vsdocsdocs/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/visualstudio-docs",
      "feedback_product_url": "https://developercommunity.visualstudio.com/",
      "breadcrumb_path": "/visualstudio/_breadcrumb/toc.json",
      "ROBOTS": "INDEX,FOLLOW",
      "author": "erickson-doug",
      "ms.author": "douge",
      "manager": "douge",
      "ms.date": "11/16/2016",
      "ms.topic": "conceptual",
      "audience": "developer",
      "ms.prod": "visual-studio-dev15",
      "searchScope": ["VS IDE"],
      "uhfHeaderId": "MSDocsHeader-VisualStudio",
      "titleSuffix": "Visual Studio"
    },

    "fileMetadata": {
      "breadcrumb_path": 
      {
        "**/*.md": "_breadcrumb/toc.json",
        "vs-2015/**/*.md": "vs-2015/_breadcrumb/toc.json"
      }
    },

    "template": [],
    "xref": ["missingapi.yml", "vs.140.zip", "office15.zip"],
    "dest": "vsdocsdocs",
    "groups": {
      "group-vs": {
        "moniker-range": ">= vs-2017",
        "dest": "vsdocsdocs-2017"
      },
      "group-vs2015": {
        "moniker-range": "vs2015",
        "dest": "vsdocsdocs-2015"
      }
    }
  }
}
