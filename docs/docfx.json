{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "group": "group-vs",
        "src": "~",
        "exclude": [
          "**/obj/**",
          "vsdocsdocs/**",
          "**/includes/**",
          "missingapi.yml"
        ]
      }
        ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "group": "group-vs",
        "src": "~",
        "exclude": [
          "**/obj/**",
          "vsdocsdocs/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/visualstudio-docs",
      "feedback_product_url": "https://developercommunity.visualstudio.com/",
      "breadcrumb_path": "/visualstudio/_breadcrumb/toc.json",
      "ROBOTS": "INDEX,FOLLOW",
      "author": "erickson-doug",
      "ms.author": "douge",
      "manager": "douge",
      "ms.date": "11/16/2016",
      "ms.topic": "conceptual",
      "audience": "developer",
      "ms.prod": "visual-studio-dev15",
      "searchScope": ["VS IDE"],
      "uhfHeaderId": "MSDocsHeader-VisualStudio",
      "titleSuffix": "Visual Studio",
      "monikerRange": ">= vs-2017",
      "versions": {
         "vs-2015": 
         {
           "src": "docs/vs-2015",
           "toc_rel": "docs/vs-2015/TOC.md",
           "breadcrumb_path": "docs/vs-2015/_breadcrumb/toc.json"
         },
         ">= vs-2017": 
         {
           "src": "docs",
           "toc_rel": "docs/TOC.md",
           "breadcrumb_path": "docs/_breadcrumb/toc.json"
         }
      }

    },

    "fileMetadata": {
      "monikerRange": {
        "vs-2015/**/*.md": "vs-2015"
      }
    },


    "template": [],
    "xref": ["missingapi.yml", "vs.140.zip", "office15.zip"],
    "dest": "vsdocsdocs",
    "group": {
      "group-vs": {
        "moniker-range": "vs-2015 || >= vs-2017"
      }
    }
  }
}
